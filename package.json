{
    "name": "holidaze",
    "version": "1.0.0",
    "description": "Holidaze",
    "main": "index.js",
    "author": "Raymond Johannessen",
    "license": "MIT",
    "scripts": {
        "start": "yarn run relay && react-scripts start",
        "build": "yarn run relay && react-scripts build",
        "relay": "yarn run relay-compiler --src ./src --schema ./src/relay/schema.graphql --language typescript",
        "eject": "react-scripts eject",
        "getGQLSchema": "get-graphql-schema http://localhost:8080/graphql > ./src/relay/schema.graphql"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "lodash": "^4.17.15",
        "react": "^0.0.0-experimental-e5d06e34b",
        "react-dom": "^0.0.0-experimental-e5d06e34b",
        "react-helmet-async": "^1.0.5",
        "react-relay": "^0.0.0-experimental-8cc94ddc",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.4.1",
        "relay-runtime": "^9.0.0",
        "styled-components": "^5.1.0",
        "styled-normalize": "^8.0.7"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/lodash": "^4.14.150",
        "@types/node": "^13.13.4",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-helmet-async": "^1.0.3",
        "@types/react-relay": "^7.0.3",
        "@types/react-router-dom": "^5.1.5",
        "@types/relay-runtime": "^8.0.8",
        "@types/styled-components": "^5.1.0",
        "@typescript-eslint/eslint-plugin": "^2.29.0",
        "@typescript-eslint/parser": "^2.29.0",
        "babel-plugin-relay": "^9.0.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "get-graphql-schema": "^2.1.2",
        "graphql": "^15.0.0",
        "prettier": "^2.0.5",
        "relay-compiler": "^9.0.0",
        "relay-compiler-language-typescript": "^12.0.1",
        "typescript": "^3.8.3"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
